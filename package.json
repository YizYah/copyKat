{
  "name": "copykat",
  "version": "1.0.0",
  "description": "pounce on a great code base and build a generator based on it",
  "main": "lib/index.ts",
  "scripts": {
    "lint": "eslint . --ext .ts --config .eslintrc",
    "lintfix": "eslint . --ext .ts --config .eslintrc --fix",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "keywords": [
    "oclif"
  ],
  "author": "YizYah",
  "license": "MIT",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.50",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^7.1.2",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "bin": {
    "copykat": "./bin/run"
  },
  "bugs": "https://github.com/YizYah/copykat/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/YizYah/copykat",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "copykat",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "YizYah/copykat",
  "types": "lib/index.d.ts"
}
